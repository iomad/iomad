{{!
    @template core/actions

    Actions.

    Example context (json):
    {
        "actions": [{"event": "event", "jsfunction": "Y.log", "id": "id"}]
    }
}}
{{#js}}
    require(['core/yui'], function(Y) {
        {{#actions}}
            Y.on('{{event}}', {{{jsfunction}}}, '#{{id}}', null{{#jsfunctionargs}}, {{{jsfunctionargs}}}{{/jsfunctionargs}});
        {{/actions}}
    });
{{/js}}
